pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Kod derleniyor...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testler çalıştırılıyor...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy ediliyor...'
            }
        }
    }
}
